{
  "name": "@sliceapi/sdk",
  "version": "0.0.1",
  "description": "TypeScript SDK for SliceAPI",
  "private": false,
  "type": "module",
  "main": "./dist/sdk/src/index.js",
  "types": "./dist/sdk/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/sdk/src/index.d.ts",
      "import": "./dist/sdk/src/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node --esm src/index.ts",
    "test": "tsx test.ts",
    "test:watch": "tsx watch test.ts",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "licensing",
    "laas",
    "sdk",
    "typescript"
  ],
  "author": "Rohan Puri <xpydrrr@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xpydr/slice.git",
    "directory": "sdk"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "dotenv": "^17.2.3",
    "ts-node": "^10.9.1",
    "tsx": "^4.21.0",
    "typescript": "^5.1.0"
  }
}
