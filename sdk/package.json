{
  "name": "@slice/sdk",
  "version": "0.0.1",
  "description": "TypeScript SDK for SliceAPI",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node --esm src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "licensing",
    "laas",
    "sdk",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.1.0",
    "ts-node": "^10.9.1"
  }
}
